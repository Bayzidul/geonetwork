<?xml version="1.0" encoding="ISO-8859-1"?>

<intermap>
	<!-- ====================================================================== -->

	<general>
		<profiles>user-profiles.xml</profiles>
		<debug>true</debug>
		<uploadDir>WEB-INF/data/tmp</uploadDir>
		<useProxy>false</useProxy>
		<proxyHost>10.11.0.1</proxyHost>
		<proxyPort>3128</proxyPort>
