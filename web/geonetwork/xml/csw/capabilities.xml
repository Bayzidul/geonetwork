<?xml version="1.0" encoding="UTF-8" ?>

<csw:Capabilities version="2.0.1"
						xmlns:ows  ="http://www.opengis.net/ows"
						xmlns:ogc  ="http://www.opengis.net/ogc"
						xmlns:csw  ="http://www.opengis.net/cat/csw"
