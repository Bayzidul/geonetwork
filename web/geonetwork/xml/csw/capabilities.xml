		<!-- Parameters -->
		<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - -  -->	
		
		<ows:Parameter name="service">
			<ows:Value>http://www.opengis.net/cat/csw</ows:Value>
		</ows:Parameter>
		
		<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - -  -->	
		
