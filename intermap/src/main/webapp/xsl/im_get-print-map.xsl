<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<!-- Main template --><xsl:template match="/">	<html>		<head>			<title>				<xsl:value-of select="/root/strings/title" />			</title>			<link rel="stylesheet" type="text/css" href="../../intermap.css" />		</head>		<body>			<table>				<!-- Map -->				<tr>					<td>						<img src="{/root/response/url}" />					</td>				</tr>				<!-- Legend -->				<tr>					<td>						<table>							<tr>								<td>									<xsl:call-template name="legends" />								</td>							</tr>						</table>					</td>				</tr>			</table>		</body>	</html></xsl:template><!-- Legends --><xsl:template name="legends">	<xsl:for-each select="/root/response/legends/legend">		<img src="{.}" />		<xsl:if test="position() mod 3 = 0">			<br />		</xsl:if>	</xsl:for-each></xsl:template></xsl:stylesheet>