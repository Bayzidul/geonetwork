<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"xmlns:xlink="http://www.w3.org/1999/xlink"><!--<xsl:include href="banner.xsl"/>--><xsl:template match="/">	<html>		<head>			<title>				<xsl:value-of select="/root/strings/title" />			</title>			<link rel="stylesheet" type="text/css" href="../../intermap.css" />		</head>		<body onLoad="javascript:window.focus();"><!--			<xsl:apply-templates select="/root/gui/banner" />--><!--			<img src="{/root/response/service/response/ARCXML/RESPONSE/IMAGE/LEGEND/@url}" />--><!--			<img src="{/root/response/legendUrl}" />-->			<xsl:choose>				<xsl:when test="/root/response/service/@type = 'ArcIMS'">					<img src="{/root/response/service/response/ARCXML/RESPONSE/IMAGE/LEGEND/@url}" />				</xsl:when>				<xsl:when test="/root/response/service/@type = 'WMS'">					<img src="{//Layer[Name=/root/response/service/@name]/ancestor-or-self::Layer/Style[Name=/root/response/service/@style]/LegendURL/OnlineResource/@xlink:href}" />				</xsl:when>			</xsl:choose>		</body>	</html></xsl:template></xsl:stylesheet>