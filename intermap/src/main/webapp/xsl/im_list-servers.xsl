<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">		<xsl:include href="main.xsl"/>		<xsl:template match="/">		<html>			<head>				<title>					<xsl:value-of select="/root/gui/strings/title" />				</title>				<link rel="stylesheet" type="text/css" href="../../intermap.css" />				<script language="javascript">					function skip() { this.blur(); }										function dont() { return false; }										function disableTextField(field) {						field.value = "";												if (document.all || document.getElementById)							field.disabled = true;						else {							field.onfocus = skip;							field.onkeypress = dont;						}					}										function enableTextField(field) {						if (document.all || document.getElementById)							field.disabled = false;						else {							field.onfocus = null;							field.onkeypress = null;						}        				field.focus();					}										function toggleTextField(value, field) {						if (value &lt; 0) { enableTextField(field); }						else { disableTextField(field); }					}										function setTextField(value, field) {						toggleTextField(value, field);						if (value == "-1") {field.value="http://&lt;servername&gt;/servlet/com.esri.esrimap.Esrimap";}						if (value == "-2") {field.value="";}					}										function getMenuValue(menu) {						return menu[menu.selectedIndex].value					}									function showFAOOWS() {						window.open("http://www.fao.org/geonetwork/ows.html?host=" + location.host + "&amp;service=<xsl:value-of select='/root/gui/locService'/>/map.addServicesExt", "FAOOWS", "HEIGHT=500,WIDTH=700,scrollbars=yes,toolbar=yes,status=yes,menubar=yes,location=yes,resizable=yes");					}				</script>			</head>			<body onLoad="setTextField(getMenuValue(document.form.mapserver), document.form.url)">				<xsl:call-template name="formLayout">					<xsl:with-param name="title" select="/root/gui/strings/choosemapserver"/>					<xsl:with-param name="content">						<xsl:copy-of select="/root/gui/strings/feedbackTopics"/>						<p/>						<form action="{/root/gui/locService}/mapServers.getServices" method="post" name="form">							<table>								<tr>									<th class="padded"><xsl:value-of select="/root/gui/strings/mapServer" /></th>									<td class="padded">										<select name="mapserver" onChange="setTextField(getMenuValue(this), this.form.url)">											<xsl:apply-templates select="/root/response/mapServers/server" />											<option value="-2">												<xsl:value-of select="/root/gui/strings/otherWMS" />											</option>										</select>									</td>								</tr>								<tr>									<th class="padded"><xsl:value-of select="/root/gui/strings/specify"/></th>									<td class="padded"><input class="content" type="text" name="url" size="60"/></td>								</tr>								<tr><td><br/></td></tr>								<tr>									<th class="padded">										<ul type="square">											<li>												<span onclick="javascript:showFAOOWS();" title="Open a list of OGC Web Map Services from &#10;&#13; the Food and Agriculture Organization (FAO)">FAO Web Map Services</span>											</li>										</ul>									</th>									<td class="padded">										<a href="javascript:showFAOOWS();" title="Open a list of OGC Web Map Services &#10;&#13; from the Food and Agriculture Organization (FAO)">Click here</a>									</td>								</tr>								<tr><td><br/></td></tr>							</table>						</form>					</xsl:with-param>					<xsl:with-param name="buttons">						<button class="content" onclick="document.form.action='{/root/gui/locService}/map.getMain';document.form.submit();"><xsl:value-of select="/root/gui/strings/cancel"/></button>						&#160;						<button class="content" onclick="document.form.submit();"><xsl:value-of select="/root/gui/strings/connect"/></button>					</xsl:with-param>				</xsl:call-template>			</body>		</html>	</xsl:template>	<!-- Servers menu options -->	<xsl:template match="/root/response/mapServers/server">		<option value="{@id}"><xsl:value-of select="@name" /></option>	</xsl:template>	<!-- Types menu options -->	<xsl:template match="/root/response/types/type">		<option value="{@id}"><xsl:value-of select="." /></option>	</xsl:template></xsl:stylesheet>